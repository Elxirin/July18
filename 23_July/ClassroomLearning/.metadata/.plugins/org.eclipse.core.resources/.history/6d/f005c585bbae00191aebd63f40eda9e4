package Capgemini.oops;

class Subject {

	String[] subject;
	
	
	Subject(String sub1, String sub2, String sub3){
		subject[0]=sub1;
		subject[1]=sub2;
		subject[2]=sub3;
		
	}
}
class Marks {
	double[] marks;
	
	Marks( double m1, double m2, double m3){
		marks[0]=m1;
		marks[1]=m2;
		marks[2]=m3;
	}
	
}